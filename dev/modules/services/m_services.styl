.pricelist
  position absolute
  left 0
  bottom 15px
  width 100%
  text-align center
  @media screen and (min-width: 360px) and (min-height: 660px)
    bottom 40px
  @media screen and (min-width: 1024px) and (min-height: 1000px)
    bottom 50px
  @media screen and (min-width: 1240px)
    bottom 70rem
  a
    color #b0914f
    display inline-block
    vertical-align middle
    padding-left 35px
    position relative
    &:after
      content ''
      background: url('../images/sprite.png') no-repeat -100px -50px;
      width: 24px;
      height: 24px;
      position absolute
      left 0
      top 50%
      margin-top -12px
      retina()
    span
      text-decoration underline
    &:hover
      color #fff
      &:after
        background-position -150px -50px
.m_services
  position relative
  z-index 40
  .text
    position relative
    z-index 40
    margin-top 200px
    opacity 0
    transition opacity 1s ease-in
    transition-delay 0s
    max-width 600px
    margin-left auto
    margin-right auto
    @media screen and (min-width: 768px) and (min-height: 1000px)
      margin-top 400px
    @media screen and (min-width: 992px) and (min-height: 750px)
      margin-top 300px
    @media screen and (min-width: 1240px)
      margin-top 450rem
    @media screen and (max-height: 520px)
      margin-top 130px
    @media screen and (max-height: 460px)
      margin-top 0
      transition opacity 1s ease-in
    section.active &
      transition opacity .5s ease-in
      transition-delay 0s
      opacity 1
    .number
      position absolute
      left 0
      top 50%
      margin-top -90px
      width 100%
      height 100%
      color #282828
      font-size 180px
      line-height 180px
      font-family "Helvetica Neue", "Arial Black", "Arial Bold", Gadget, sans-serif
      font-weight 700
      opacity 0.2
      @media screen and (min-width: 1240px)
        font-size 260rem
        line-height 260rem
        margin-top -130rem
    h2, p
      position relative
      z-index 1
      opacity 0
      transition all .5s cubic-bezier(.25,-0.55,.25,.25)
    h2
      transform translateY(-20px)
      section.section.fp-completely &
        transform translateY(0px)
        transition-delay 0s
        opacity 1
    p
      transform translateY(-20px)
      section.section.fp-completely &
        transform translateY(0px)
        transition-delay .3s
        opacity 1
    h2
      @media screen and (min-width: 768px) and (min-height: 640px)
        font-size 40px
      @media screen and (min-width: 1240px)
        font-size 48rem
    p
      margin-bottom 0
      margin-top 15px
      color #b1b1b1
      @media screen and (min-width: 768px) and (min-height: 640px)
        font-size 18px
      @media screen and (min-width: 1240px)
        font-size 18rem
// Global images
.fp-viewing-2,
.fp-viewing-3,
.fp-viewing-4,
.fp-viewing-5,
.fp-viewing-6
  #services,
  #services-nav
    transition-delay .5s
    transition opacity 1s ease-in
    transform translateX(0%)
    opacity 1
  #services-nav
    transition-delay 1s
    z-index 10
#services-nav
  transition opacity .2s ease-in
  position absolute
  left 0
  top 245px
  width 100%
  height 15px
  -webkit-backface-visibility hidden
  -webkit-perspective 1000
  // when hidden
  z-index -1
  transform translateX(-100%)
  opacity 0
  @media screen and (min-width: 360px) and (min-height: 660px)
    top 270px
  @media screen and (min-width: 410px) and (min-height: 730px)
    top 290px
  @media screen and (min-width: 768px) and (min-height: 640px)
    top 290px
  @media screen and (min-width: 768px) and (min-height: 800px)
    top 350px
  @media screen and (min-width: 768px) and (min-height: 1000px)
    top 460px
  @media screen and (min-width: 992px) and (min-height: 750px)
    top 410px
  @media screen and (min-width: 1024px) and (min-height: 1360px)
    top 550px
  @media screen and (min-width: 1240px)
    top 620rem
  @media screen and (min-width: 1240px) and (max-height: 690px)
    top 550rem
  @media screen and (min-width: 1240px) and (max-height: 600px)
    top 520rem
  @media screen and (max-height: 520px)
    top 190px
  @media screen and (max-height: 460px)
    display none
  .services-progress
    height 20px
    width 260px
    margin-left auto
    margin-right auto
    overflow hidden
    position relative
    padding-left 2.5px
    padding-right 2.5px
    @media screen and (min-width: 480px)
      width 320px
    @media screen and (min-width: 568px)
      width 380px
    @media screen and (min-width: 768px)
      width 560px
    @media screen and (min-width: 992px)
      width 740px
    @media screen and (min-width: 1200px)
      width 980px
    .progress-bar
      position absolute
      left 0
      top 0
      width 100%
      height 100%
      margin-left 0.5px
      &.anim-in-second
        .animate
          transition width 1s ease-in
          transition-delay 0
          width 25%
      &.anim-in-third
        .animate
          transition width 1s ease-in
          transition-delay 0
          width 50%
      &.anim-in-fourth
        .animate
          transition width 1s ease-in
          transition-delay 0
          width 75%
      &.anim-in-fifth
        .animate
          transition width 1s ease-in
          transition-delay 0
          width 100%
      .normal
        position absolute
        left 0
        top 0
        width 100%
        height 100%
        z-index 1
        background url("../images/serv-dots-normal.png") repeat-x 21px 50%
      .animate
        transition width 1.2s ease-in
        position absolute
        left 0
        top 0
        height 100%
        z-index 2
        background url("../images/serv-dots-hover.png") repeat-x 21px 50%
        width 0
        body.fp-viewing-2 &
          width 0
        body.fp-viewing-3 &
          width 25%
        body.fp-viewing-4 &
          width 50%
        body.fp-viewing-5 &
          width 75%
        body.fp-viewing-6 &
          width 100%

    ul
      clearfix()
      width 300px
      height 100%
      display inline-block
      vertical-align top
      position relative
      @media screen and (min-width: 480px)
        width 375px
      @media screen and (min-width: 568px)
        width 450px
      @media screen and (min-width: 768px)
        width 675px
      @media screen and (min-width: 992px)
        width 900px
      @media screen and (min-width: 1200px)
        width 1200px
      li
        position relative
        float left
        width 60px
        height 100%
        @media screen and (min-width: 480px)
          width 75px
        @media screen and (min-width: 568px)
          width 90px
        @media screen and (min-width: 768px)
          width 135px
        @media screen and (min-width: 992px)
          width 180px
        @media screen and (min-width: 1200px)
          width 240px
        &#s-projecting a
          left 0
        &#s-design a
          left 0
        &#s-webdev a
          left 0
        &#s-mobdev a
          left 0
        &#s-backend a
          left 0
        a
          transition all .5s ease-in
          transition-delay .8s
          position absolute
          top 50%
          display block
          width 15px
          height 15px
          margin-top -(@height/2)
          border 1px solid #fff
          border-radius 50%
          z-index 3
          box-shadow inset 0 0 0 4px #111
          background #fff
          @media screen and (min-width: 1200px)
            box-shadow inset 0 0 0 4px #111, 0 0 0 12px #111
          &:hover,
          &.current
            border-color #d1ab59
            background #d1ab59
          &:hover
            transition all .5s ease-in
            transition-delay 0
          &.current
            border-width 2px
            width 20px
            height 20px
            margin-top -10px
            margin-left -2.5px

#services
  transition opacity .2s ease-in
  position absolute
  left 0
  top 0
  width 100%
  height 225px
  -webkit-backface-visibility hidden
  -webkit-perspective 1000
  margin-top 50px
  // when hidden
  // z-index -1
  // opacity 0
  // transform translateX(-100%)
  @media screen and (min-width: 768px) and (min-height: 640px)
    margin-top 75px
  @media screen and (min-width: 768px) and (min-height: 1000px)
    margin-top 150px
  @media screen and (min-width: 992px) and (min-height: 750px)
    margin-top 75px
  @media screen and (min-width: 1024px) and (min-height: 1000px)
    margin-top 100px
  @media screen and (min-width: 1240px)
    margin-top 150rem
  @media screen and (max-height: 520px)
    height 120px
  @media screen and (max-height: 460px)
    display none
  .serv-graphs
    width 100%
    height 100%
    .services-images
      height 185px
      width 100%
      text-align center
      position relative
      @media screen and (min-width: 768px) and (min-height: 800px)
        height 250px
      @media screen and (min-width: 992px) and (min-height: 750px)
        height 300px
      @media screen and (min-width: 1240px)
        height 400rem
      @media screen and (min-width: 1240px) and (max-height: 690px)
        height 350rem
      @media screen and (max-height: 520px)
        height 120px
      .item
        transition-delay 1s
        transition opacity 1s ease-in
        width 180px
        height 180px
        display inline-block
        vertical-align middle
        position absolute
        left 50%
        top 50%
        margin-left -(@width/2)
        margin-top -(@height/2)
        opacity 0
        @media screen and (min-width: 768px) and (min-height: 800px)
          width 240px
          height 240px
          margin-left -120px
          margin-top -120px
        @media screen and (min-width: 992px) and (min-height: 750px)
          width 300px
          height 300px
          margin-left -150px
          margin-top -150px
        @media screen and (min-width: 1240px)
          width 400rem
          height 400rem
          margin-left -200rem
          margin-top -200rem
        @media screen and (min-width: 1240px) and (max-height: 690px)
          width 350rem
          height 350rem
          margin-top -175rem
          margin-left -175rem
        @media screen and (max-height: 520px)
          height 120px
          width 120px
          margin-left -60px
          margin-top -60px
        body.fp-viewing-2 &
          &:nth-child(1)
            opacity 1
        body.fp-viewing-3 &
          &:nth-child(2)
            opacity 1
        body.fp-viewing-4 &
          &:nth-child(3)
            opacity 1
        body.fp-viewing-5 &
          &:nth-child(4)
            opacity 1
        body.fp-viewing-6 &
          &:nth-child(5)
            opacity 1
